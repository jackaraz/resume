<main class="stack pad-horizontal pad-vertical">
  {% for section in order.sections %}
    {% set title = section.title %}
    {% if section.id === 'skills' %}
      {% set listSection = skills %}
     {% include './sectionSkills.njk'%}
    {% else %}
      {% set collection = collections[section.id] %}
      {% if collection.length > 0 %}
        {% include './sectionEvents.njk'%}
      {% endif %}
    {% endif %}
  {% endfor %}
</main>
