<main class="stack pad-horizontal pad-vertical">
  {% for section in main.sections %}
    {% if section.isList %}
      {% set listSection = section %}
      {% include './sectionSkills.njk'%}
    {% else %}
    {# order items from end date (consider ongoing if no end date) #}
      {% set collection = section.entries | sort(attr='end_date') %}
      {% set title = section.title %}
      {% include './sectionEvents.njk'%}
    {% endif %}
  {% endfor %}
</main>
